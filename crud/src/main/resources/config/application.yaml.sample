spring:
  datasource:
    url: jdbc:mysql://localhost/java_crud?createDatabaseIfNotExist=true&autoReconnect=true&useSSL=false
    username: <your-db-user>
    password: <your-db-password>
    driverClassName: com.mysql.cj.jdbc.Driver
  jpa:
    hibernate:
      ddl-auto: update
  messages:
    basename: "i18n/messages"
    cache-duration: -1
    encoding: UTF-8
